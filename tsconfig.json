{
	"compilerOptions": {
		"strict": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "es2019",
		"lib": ["es2019", "es2020", "es2022.error", "dom"],
		"removeComments": true,
		"useUnknownInCatchVariables": false,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"strictNullChecks": true,
		"preserveConstEnums": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"incremental": true,
		"declaration": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"outDir": "./dist/"
	},
	"include": ["nodes/**/*", "nodes/**/*.json", "package.json"],
	"exclude": [
		"node_modules/**/*",
		"node_modules/@langchain/core/node_modules/zod/**/*",
		"node_modules/zod/dist/types/v4/**/*"
	]
}
